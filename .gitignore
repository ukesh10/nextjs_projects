# Build output
build/                # React/Vite build output
dist/                 # Vite build output (also used in other tools)
.next/                # Next.js build output

# Next.js specific
out/                  # Next.js static export output

# Vite specific
.vite/                # Vite caching folder

# Environment files
.env                  # Environment variables
.env.local            # Local environment variables
.env.*.local          # Local environment files for different environments

# OS and Editor specific files
.vscode/              # VS Code settings and extensions
.idea/                # JetBrains IDE settings
Thumbs.db             # Windows image cache file
.vs/                  # Visual Studio settings folder

# Editor History files
.history/             # VSCode Local History files

# Temporary and backup files
*.swp                 # Vim swap files
*.swo                 # Vim backup files
*~                    # Backup files

# Yarn lock file (if you're not using Yarn)
yarn.lock

# .eslintcache (if using ESLint)
.eslintcache

# Vite / Webpack
.cache/               # Cache from Vite or other bundlers (Webpack, etc.)

# Storybook (if using it)
.storybook/

# React Native (optional, if using)
.expo/
